    "@radix-ui/react-tooltip": "^1.2.7",
    "@tanstack-query-firebase/react": "^2.0.0",
    "@tanstack/react-query": "^5.80.5",
    "@types/ws": "^8.18.1",
    "@xenova/transformers": "^2.17.2",
    "acorn": "^8.14.1",
    "acorn-jsx": "^5.3.2",
    "acorn-typescript": "^1.4.13",
    "better-sqlite3": "^9.0.0",
    "chokidar": "^4.0.3",
