    "@next/bundle-analyzer": "^15.3.3",
    "@types/better-sqlite3": "^7.6.8",
    "@types/chokidar": "^2.1.7",
    "@types/node": "^22.15.30",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "@types/vscode": "^1.100.0",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "autoprefixer": "^10.4.21",
    "browserslist": "^4.25.0",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.28.0",
    "eslint-config-next": "^15.3.3",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-unused-imports": "^4.1.4",
    "genkit-cli": "^1.11.1",
    "handlebars": "^4.7.8",
    "handlebars-loader": "^1.7.3",
    "lint-staged": "^15.2.10",
    "patch-package": "^8.0.0",
    "postcss": "^8.5.4",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "rimraf": "^5.0.10",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "vscode": "^1.1.37"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": "20",
    "npm": ">=8.0.0"
  },
  "overrides": {
    "minimist": "^1.2.8",
    "minimatch": "^3.1.2",
    "mkdirp": "^1.0.4"
  },
  "keywords": [
    "ai",
    "automation",
    "developer-tools",
    "workflow",
    "nextjs",
    "typescript",
    "claude",
    "openai"
  ],
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ]
}
