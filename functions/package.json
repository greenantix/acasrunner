    "next": "^15.3.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "firebase-functions-test": "^3.1.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  }
